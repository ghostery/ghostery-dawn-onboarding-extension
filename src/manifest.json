{
  "version": "1.1.1",
  "browser_specific_settings": {
    "gecko": {
      "id": "dawn-onboarding@ghostery.com"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2,
  "name": "Ghostery Private Browser Onboarding",
  "permissions": [
    "storage",
    "webNavigation",
    "search"
  ],
  "experiment_apis": {
    "ghostery": {
      "schema": "experiment_apis/schema.json",
      "parent": {
        "scopes": ["addon_parent"],
        "paths": [["ghostery"]],
        "script": "experiment_apis/parent.js"
      }
    }
  },
  "default_locale": "en"
}
